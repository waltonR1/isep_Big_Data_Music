# Airflow 配置
AIRFLOW_UID=50000# Airflow 容器运行用户 UID（建议保留默认值）
AIRFLOW_GID=0# Airflow 容器运行用户 GID（通常为 0）
AIRFLOW__CORE__LOAD_EXAMPLES=True# 是否加载示例 DAG，生产环境应设为 False
AIRFLOW__WEBSERVER__SECRET_KEY=your_secret_key_here# Web UI 的加密密钥，应替换为安全的随机字符串
_AIRFLOW_WWW_USER_USERNAME=your_admin_username# 初始 Web UI 管理员用户名
_AIRFLOW_WWW_USER_PASSWORD=your_admin_password# 初始 Web UI 管理员密码

# Spotify API 配置（从 https://developer.spotify.com 申请）
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here
